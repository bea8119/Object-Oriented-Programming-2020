package groups;

public class Bid {
	
	private Group group;
	private Supplier supplier;
	private int price;
	public Bid(Group group, Supplier supplier, int price) {
	
		this.group = group;
		this.supplier = supplier;
		this.price = price;
	}
	public Group getGroup() {
		return group;
	}
	public Supplier getSupplier() {
		return supplier;
	}
	public int getPrice() {
		return price;
	}
	
	
	

}
