package university;

	 class Students {
		 public String name;
		 public String lastn;
		 public int  ID;
		 public Courses[] listCourses = new Courses[25];
		 public int numcourses = 0;
		 
		public Students(String name, String lastn, int iD) {
			this.name = name;
			this.lastn = lastn;
			ID = iD;
		}
		 
		public addCourse(String title, String teacher, int id) {
			Courses course = new Courses(, teacher, 10+numcourses);
			course.title= title;
			course.teacher = teacher;
			course.id = id;
			listCourses[numcourses++] = course;
		}
		

		
		
		
		
		 
		

}
